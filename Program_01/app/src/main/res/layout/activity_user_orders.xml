<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserOrders">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="431dp"
        android:layout_height="0dp"
        android:layout_marginBottom="251dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/btn_emergency_towing"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/header"
        tools:layout_editor_absoluteX="-10dp" />

    <ImageView
        android:id="@+id/btn_v_uo_back"
        android:layout_width="52dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/btn_v_uo_home"
        android:layout_width="137dp"
        android:layout_height="75dp"
        android:layout_marginStart="136dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home_icon" />

    <ImageView
        android:id="@+id/btn_v_uo_profile"
        android:layout_width="137dp"
        android:layout_height="75dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/user_cicrle_duotone_line" />

    <ImageView
        android:id="@+id/btn_v_uo_order"
        android:layout_width="137dp"
        android:layout_height="75dp"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ticket_alt" />
</androidx.constraintlayout.widget.ConstraintLayout>